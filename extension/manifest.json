{
  "manifest_version": 2,
  "name": "torafugu",
  "version": "1.0",
  "description": "Edit while translating",
  "permissions": [
    "<all_urls>",
    "menus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
